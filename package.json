{
  "name": "@n3wth/ui",
  "version": "0.2.2",
  "description": "Flat, minimal, iOS-inspired React design system with glass morphism components and Tailwind CSS tokens.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/styles.css",
    "./tailwind": "./tailwind.preset.js"
  },
  "files": [
    "dist",
    "tailwind.preset.js",
    "public"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "demo": "vite --config vite.demo.config.ts",
    "demo:build": "vite build --config vite.demo.config.ts",
    "build": "tsc && vite build && node -e \"const fs=require('fs');const css=fs.readFileSync('src/styles.css','utf8').replace(/@import 'tailwindcss';\\n\\n/,'');fs.writeFileSync('dist/styles.css',css)\"",
    "lint": "eslint src",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.5.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^9.39.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.0"
  },
  "keywords": [
    "design-system",
    "react",
    "tailwind",
    "atomic-design",
    "glass-morphism",
    "design-tokens",
    "component-library",
    "ios-inspired"
  ],
  "homepage": "https://ui.newth.ai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/n3wth/n3wth-ui"
  }
}
